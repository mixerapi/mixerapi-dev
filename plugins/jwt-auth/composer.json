{
    "name": "mixerapi/jwt-auth",
    "description": "A JWT authentication plugin for CakePHP.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "keywords": ["cakephp", "jwt", "cakephp jwt", "cakephp jwt auth", "cakephp jwt authentication"],
    "require": {
        "ext-openssl": "*",
        "php": "^8.0",
        "cakephp/authentication": "2.9.*",
        "cakephp/cakephp": "^4.2",
        "firebase/php-jwt": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "MixerApi\\JwtAuth\\": "src/"
        }
    },
    "support": {
        "source": "https://github.com/mixerapi/jwt-auth"
    },
    "authors": [
        {
            "name": "MixerApi",
            "role": "Organization"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
